{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "no-var": 0,
    "prefer-template": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
    "max-len": [2, 100],
    "no-console":0,
      "prefer-arrow-callback": 0,
      "func-names": 0,
      "prefer-const": 0,
      "no-param-reassign": 0,
      "no-unused-vars": ["error", { "args": "none" }],
      "quote-props": 0,
      "padded-blocks": ["error", { "classes": "always" }],
      "no-underscore-dangle": 0,
      "object-shorthand": 0,
      "newline-per-chained-call": ["error", { "ignoreChainWithDepth": 4 }],
      "no-prototype-builtins": 0,
      "vars-on-top": 0
  },
  "globals": {
    "angular": false,
    "Twilio": false,
    "app": false,
    "moment": false
  }
}